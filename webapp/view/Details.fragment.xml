<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:f="sap.ui.layout.form">
	<Dialog
		id="DetailsDialog"
		title="{i18n>dialogTitle}">
		<content>
		<f:SimpleForm id="simpleFormChange" title="{i18n>formTitle}" editable="true" class="editableForm sapUiSmallMarginTopBottom">
			<f:content>
				<Label text="{i18n>layoutFormMaterialNumber}" labelFor="p_materialNumber"/>
				<Input value="{MaterialNr}" id="p_materialNumber"/>
				<Label text="{i18n>layoutFormPlant}"/>
				<Input value="{Plant}" id="p_plant"/>
				<Label text="{i18n>layoutFormRevision}"/>
				<Input value="{Revision}" id="p_revision"/>
			</f:content>
		</f:SimpleForm>
		</content>
		<beginButton>
			<Button id="Continue" text="{i18n>btnContinue}" press="onContinue" type="Accept" />
		</beginButton>
	</Dialog>
</core:FragmentDefinition>