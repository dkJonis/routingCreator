{
	"_version": "1.5.0",
	"sap.app": {
		"id": "com.flexso.routingbuilder",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZPPC_CDS_ROUTING_CDS": {
				"uri": "/sap/opu/odata/sap/ZPPC_CDS_ROUTING_CDS/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"ZPPC_CDS_TEMPLATE_CDS": {
				"uri": "/sap/opu/odata/sap/ZPPC_CDS_TEMPLATE_CDS/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZPPC_CDS_TEMPLATE_CDS/metadata.xml"
				}
			},
			"ZPPC_CDS_WORKCENTER_CDS": {
				"uri": "/sap/opu/odata/sap/ZPPC_CDS_WORKCENTER_CDS/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZPPC_CDS_WORKCENTER_CDS/metadata.xml"
				}
			},
			"ZROUTING_CREATOR_SRV": {
				"uri": "/sap/opu/odata/sap/ZROUTING_CREATOR_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZROUTING_CREATOR_SRV/metadata.xml"
				}
			},
			"ZBAPI_TEST_SRV": {
				"uri": "/sap/opu/odata/sap/ZBAPI_TEST_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZBAPI_TEST_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.flexso.routingbuilder.view.Overview",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.collaboration": {},
				"sap.f": {},
				"sap.m": {},
				"sap.ui.comp": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.flexso.routingbuilder.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "Default",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZROUTING_CREATOR_SRV",
				"preload": true
			},
			"routingCreate": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZBAPI_TEST_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"controlId": "app",
				"viewPath": "com.flexso.routingbuilder.view"
			},
			"targets": {
				"Overview": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Overview",
					"viewLevel": 1
				}
			},
			"routes": [{
				"name": "Overview",
				"pattern": "/{details}",
				"titleTarget": "",
				"greedy": false,
				"target": ["Overview"]
			}]
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}